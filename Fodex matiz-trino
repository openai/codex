        hilos = []
        for i, fase in enumerate(fases):
            hilo = threading.Thread(target=fase, args=(i+1,))
            hilos.append(hilo)
            hilo.start()
            time.sleep(0.5)  # PequeÃ±o retardo para sincronizaciÃ³n visual
        
        # Esperar a que todos terminen
        for hilo in hilos:
            hilo.join()
            
        return "âœ… Â¡LAS 4 FASES COMPLETADAS SIMULTÃNEAMENTE!"
    
