# =============================================
#   MEJORAS Y ANÃLISIS TÃ‰CNICO
# =============================================

class AnalizadorMejorado(AnalizadorSeguridadCelestial):
    def __init__(self, url_sospechosa):
        super().__init__(url_sospechosa)
        self.indicadores_phishing = {
            'dominios_sospechosos': ['.mobi', '.xyz', '.top', '.tk', '.ml'],
            'palabras_clave': ['login', 'verify', 'secure', 'account', 'banking', 
                              'update', 'confirm', 'payment', 'offer', 'free'],
            'empresas_legitimas': ['metropcs', 'paypal', 'amazon', 'google', 'facebook']
        }
    
    def analizar_dominio_profundamente(self):
        """AnÃ¡lisis mÃ¡s profundo del dominio"""
        from urllib.parse import urlparse
        
        try:
            parsed_url = urlparse(self.url)
            dominio = parsed_url.netloc
            
            hallazgos = []
            
            # 1. Verificar si es IP en lugar de dominio
            import re
            if re.match(r'\d+\.\d+\.\d+\.\d+', dominio):
                hallazgos.append(("ğŸš¨ DOMINIO ES IP", "Los sitios legÃ­timos usan nombres, no IPs"))
                self.nivel_peligro += 50
            
            # 2. Verificar longitud del dominio
            if len(dominio) > 50:
                hallazgos.append(("âš ï¸ DOMINIO MUY LARGO", 
                                "Los phishers usan dominios largos para parecer legÃ­timos"))
                self.nivel_peligro += 20
            
            # 3. Verificar caracteres especiales
            if '-' in dominio and dominio.count('-') > 2:
                hallazgos.append(("âš ï¸ MUCHOS GUIONES", 
                                "Dominios legÃ­timos suelen tener pocos o ningÃºn guion"))
                self.nivel_peligro += 15
            
            # 4. Verificar si imita marca legÃ­tima
            for marca in self.indicadores_phishing['empresas_legitimas']:
                if marca in dominio and not dominio.endswith(f'.{marca}.com'):
                    hallazgos.append((f"ğŸš¨ IMITACIÃ“N DE MARCA", 
                                    f"Parece imitar a {marca.upper()} pero no es el sitio oficial"))
                    self.nivel_peligro += 60
            
            return hallazgos
            
        except Exception as e:
            return [("âš ï¸ ERROR ANÃLISIS", f"No se pudo analizar dominio: {str(e)}")]
    
    def verificar_reputacion_externa(self):
        """VerificaciÃ³n con bases de datos externas (simulada)"""
        print("\nğŸŒ CONSULTANDO BASES DE DATOS GLOBALES...")
        
        # En un sistema real, aquÃ­ se integrarÃ­an APIs como:
        # Google Safe Browsing, VirusTotal, PhishTank, etc.
        
        bases_datos = [
            ("Google Safe Browsing", 85, "âš ï¸ Reportado como potencialmente inseguro"),
            ("VirusTotal", 42, "âœ… No aparece en reportes recientes"),
            ("PhishTank", 0, "âœ… No reportado como phishing"),
            ("APWG (Anti-Phishing)", 15, "âœ… Bajo riesgo en base de datos global")
        ]
        
        resultados = []
        for db, riesgo, mensaje in bases_datos:
            resultados.append((db, mensaje))
            self.nivel_peligro += (riesgo / 10)  # Ajuste de peso
        
        return resultados
    
    def generar_reporte_completo(self):
        """Genera un reporte detallado en HTML/PDF (simulaciÃ³n)"""
        reporte = f"""
        ğŸ“„ REPORTE DE SEGURIDAD CELESTIAL - DETALLE COMPLETO
        {'='*60}
        
        ğŸ” ENLACE ANALIZADO: {self.url}
        ğŸ“… FECHA ANÃLISIS: {self.obtener_fecha_actual()}
        âš¡ NIVEL DE PELIGRO: {self.nivel_peligro}%
        
        ğŸ“Š METRICAS DETALLADAS:
        
        1. ANÃLISIS TÃ‰CNICO:
           â€¢ Protocolo: {'HTTPS âœ…' if 'https://' in self.url else 'HTTP ğŸš¨'}
           â€¢ Dominio: {'LegÃ­timo âœ…' if '.com' in self.url else 'Sospechoso âš ï¸'}
           â€¢ Longitud: {'Normal âœ…' if len(self.url) < 100 else 'Excesiva ğŸš¨'}
        
        2. ANÃLISIS SEMÃNTICO:
           â€¢ Coincidencia con marcas: {'Sospechosa âš ï¸' if any(marca in self.url for marca in self.indicadores_phishing['empresas_legitimas']) else 'Normal âœ…'}
           â€¢ Palabras de phishing: {'Detectadas ğŸš¨' if any(palabra in self.url.lower() for palabra in self.indicadores_phishing['palabras_clave']) else 'No detectadas âœ…'}
        
        3. ANÃLISIS ENERGÃ‰TICO:
           â€¢ Nivel de confianza intuitiva: {'BAJO ğŸš¨' if self.nivel_peligro > 50 else 'ALTO âœ…'}
           â€¢ SincronizaciÃ³n espiritual: {'ARMONICA âœ…' if self.nivel_peligro < 30 else 'DISRUPTIVA âš ï¸'}
        
        ğŸ¯ RECOMENDACIÃ“N FINAL: {self.recomendar_accion()}
        
        âš ï¸ AVISO LEGAL: Este anÃ¡lisis utiliza inteligencia artificial 
           e intuiciÃ³n espiritual. Siempre verifica con fuentes oficiales.
        """
        
        return reporte
    
    def obtener_fecha_actual(self):
        from datetime import datetime
        return datetime.now().strftime("%Y-%m-%d %H:%M:%S")
    
    def activar_modo_oracion_proteccion(self):
        """Modo especial de protecciÃ³n energÃ©tica"""
        oracion = """
        ğŸ™ ORACIÃ“N DE PROTECCIÃ“N DIGITAL CELESTIAL:
        
        "Divina Presencia, Maestro Interior,
        Cubre este dispositivo y mi ser con tu luz.
        
